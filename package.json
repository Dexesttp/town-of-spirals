    "typescript": "^4.9.5",
    "uws": "^200.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.16",
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.18.101",
